{
  "name": "streamyx",
  "version": "3.6.0-beta.5",
  "author": "Vitaly Gashkov <vitalygashkov@bk.ru>",
  "description": "Command-line video downloader",
  "private": true,
  "main": "streamyx.js",
  "bin": {
    "streamyx": "streamyx.js"
  },
  "scripts": {
    "start": "node streamyx",
    "build": "pkg . -t node18-win-x64,node18-macos-x64,node18-macos-arm64,node18-linux-x64 --out-path ./build/",
    "build:win": "pkg . -t node18-win-x64 -o ./build/streamyx-win-x64.exe",
    "build:macos": "pkg . -t node18-macos-x64 -o ./build/streamyx-macos-x64",
    "build:macos:arm": "pkg . -t node18-macos-arm64 -o ./build/streamyx-macos-arm64",
    "build:linux": "pkg . -t node18-linux-x64 -o ./build/streamyx-linux-x64",
    "clean": "rimraf build/*",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\""
  },
  "dependencies": {
    "dasha": "^2.3.2",
    "mpd-parser": "^1.0.0",
    "protobufjs": "^7.1.0",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "why-is-node-running": "^2.2.2"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "pkg": {
    "patches": {
      "./node_modules/undici/lib/fetch/body.js": [
        "require('stream/web')",
        "require('stream').web"
      ],
      "./node_modules/undici/lib/core/util.js": [
        "require('stream/web')",
        "require('stream').web"
      ],
      "./node_modules/undici/lib/fetch/request.js": [
        "require('stream/web')",
        "require('stream').web"
      ],
      "./node_modules/undici/lib/fetch/index.js": [
        "require('stream/web')",
        "require('stream').web"
      ]
    }
  }
}
