{
  "name": "streamyx",
  "version": "3.6.1",
  "author": "Vitaly Gashkov <vitalygashkov@vk.com>",
  "description": "Command-line video downloader",
  "main": "dist/streamyx.js",
  "bin": {
    "streamyx": "dist/streamyx.js"
  },
  "scripts": {
    "start": "tsc && node dist/streamyx",
    "build": "tsc && pkg . -t node18-win-x64,node18-macos-x64,node18-macos-arm64,node18-linux-x64 --out-path ./build/",
    "build:win": "tsc && pkg . -t node18-win-x64 -o ./build/streamyx-win-x64.exe",
    "build:macos": "tsc && pkg . -t node18-macos-x64 -o ./build/streamyx-macos-x64",
    "build:macos:arm": "tsc && pkg . -t node18-macos-arm64 -o ./build/streamyx-macos-arm64",
    "build:linux": "tsc && pkg . -t node18-linux-x64 -o ./build/streamyx-linux-x64",
    "clean": "rimraf build/*",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "blowfish-node": "^1.1.3",
    "dasha": "^2.3.6",
    "mpd-parser": "^1.0.0",
    "protobufjs": "^7.1.2",
    "undici": "^5.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "pkg": "^5.8.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
